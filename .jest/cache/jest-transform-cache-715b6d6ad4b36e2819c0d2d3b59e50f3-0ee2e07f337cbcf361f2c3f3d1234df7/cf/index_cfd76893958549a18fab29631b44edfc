function makeEmptyNote() {
    return {
        beat: 0,
        rawData: '',
        type: NoteType.UNKNOWN,
        data: { arrows: makeEmptyArrows() }
    };
}
exports.makeEmptyNote = makeEmptyNote;
function makeEmptyArrows() {
